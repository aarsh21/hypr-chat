# Generate a random secret: https://generate-secret.vercel.app/32 or `openssl rand -base64 32`
AUTH_SECRET=

# Better Auth configuration
# Base URL for auth (e.g., https://yourdomain.com)
BETTER_AUTH_URL=
# Trusted origins for CORS (comma-separated, e.g., https://yourdomain.com,https://www.yourdomain.com)
BETTER_AUTH_TRUSTED_ORIGINS=

# Registration control
# Set to "true" to enable new user registration, any other value or unset disables registration
REGISTRATION_ENABLED=false

# Z.AI Coding Plan API Key
# Get your API key from https://z.ai/manage-apikey/apikey-list
# Docs: https://docs.z.ai/devpack/overview
ZHIPU_API_KEY=

# PostgreSQL database connection string
# For local PostgreSQL: postgresql://username:password@localhost:5432/database_name
POSTGRES_URL=

# Instructions to create a Redis store here:
# https://vercel.com/docs/redis
# For local Redis: redis://localhost:6379
REDIS_URL=
